{
  "name": "linopress",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^3.3.0",
    "react-scripts": "0.9.0"
  },
  "dependencies": {
    "aphrodite": "^1.1.0",
    "material-ui": "^0.17.0",
    "murmurhash": "0.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0"
  },
  "scripts": {
    "start": "concurrently --kill-others \"react-scripts start\" \"npm start --prefix packages/linopress-api\"",
    "start:dev": "concurrently --kill-others \"react-scripts start\" \"npm run dev --prefix packages/linopress-api\"",
    "frontend": "react-scripts start",
    "build": "react-scripts build && cp -r ./content ./build/api",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3001"
}
